                    <span></span> Shop
                    <span></span> Wishlist
                </div>
            </div>
        </div>
        <section class="mt-50 mb-50">
            <div class="container">
                <div class="row">
                    <%if(products.length==0){%>
                        <div class="wishlist-container mt-50 mb-50">
                            <div class="empty-wishlist">
                               
                                <h2>Your Wishlist is Empty</h2>
                                <p>Start adding items to your wishlist to keep track of your favorite things.</p>
                                <button type="button" class="btn"><a href="/allproducts" style="color:white">Add</a></button>
                            </div>
                        </div>
                    <%}else{%>
                        <div class="col-12">
                            <div class="table-responsive">
                                <table class="table shopping-summery text-center">
                                    <thead>
                                        <tr class="main-heading">
                                            <th scope="col" colspan="2">Product</th>
                                            <th scope="col">Price</th>
                                            <th scope="col">Stock Status</th>
                                            <th scope="col">Action</th>
                                            <th scope="col">Remove</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <%for(let i=0;i<products.length;i++){%>
                                            <tr>
                                                <td class="image product-thumbnail"><img src="admin/assets/imgs/products/<%=products[i].images[0].url%>" alt="<%=products[i].productName%>"></td>
                                                <td class="product-des product-name">
                                                    <h5 class="product-name"><a href="/products?id=<%=products[i]._id%>"><%=products[i].productName%></a></h5>
                                                    <p class="font-xs"><br> <%=products[i].description%>
                                                    </p>
                                                </td>
                                                <td class="price" data-title="Price"><span>₹<%=products[i].salePrice%> </span></td>
                                                <td class="text-center" data-title="Stock">
                                                    <%if(products[i].stock==0){%>
                                                        <span class="text-danger font-weight-bold">Out of stock</span>
                                                    <%}else{%>
                                                        <span class="color3 font-weight-bold">In Stock</span>
                                                    <%}%>
                                                    
                                                  
                                                </td>
                                                <td class="text-right" data-title="Cart">
                                                    <%if(products[i].stock==0){%>
                                                        <button type="button" class="btn btn-sm btn-outline-danger text-danger" disabled>Move to bag</button>
                                                   
                                                    <%}else{%>
                                                        <a href="/wishlist-to-bag?id=<%=userData.wishlist[i].productId%>" class="btn btn-sm"><i class="fi-rs-shopping-bag mr-5"></i>Move to bag</a>
                                                    <%}%>
                                                </td>
                                                <td class="action" data-title="Remove"><a href="/wishlist/deleteItem?itemId=<%=userData.wishlist[i]._id%>" class="text-muted"><i class="fi-rs-trash"></i></a></td>
                                            </tr>
                                        <%}%>
                                       
                                        
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    <%}%>
                    
                </div>
            </div>
        </section>
