<%-include("../user-layouts/mobilenav")%>
    <main class="main">
        <div class="page-header breadcrumb-wrap">
            <div class="container">
                <div class="breadcrumb">
                    <a href="/" rel="nofollow">Home</a>
                    <span></span> Pages
                    <span></span> Account
                </div>
            </div>
        </div>
        <section class="pt-50 pb-50">
            <div class="container">
                <div class="row">
                    <div class="col-lg-5 m-auto">
                        <div class="card">
                                            <div class="card-header text-center">
                                                <h5>Edit Address</h5>
                                            </div>
                                            <div class="card-body">
                                               
                                                <form method="post" action="/editAddress">
                                                    <div class="row">
                                                        <div class="form-group col-md-12">
                                                            <label>Name <span class="required">*</span></label>
                                                            <input required="" class="form-control square" name="adname" id="adname" type="text" value="<%=user[0].address[0].name%>">
                                                            <p class="error pt-1"></p>
                                                        </div>
                                                        <div class="form-group col-md-12">
                                                            <label>Mobile<span class="required">*</span></label>
                                                            <input required="" class="form-control square" name="admobile" id="admobile" type="text" value="<%=user[0].address[0].mobile%>">
                                                            <p class="error pt-1"></p>
                                                        </div>
                                                        <div class="form-group col-md-12">
                                                            <label>House Name<span class="required">*</span></label>
                                                            <input required="" class="form-control square" name="adhname" id="adhname" type="text" value="<%=user[0].address[0].housename%>">
                                                            <p class="error pt-1"></p>
                                                        </div>
                                                        <div class="form-group col-md-12">
                                                            <label>Area/Streat<span class="required">*</span></label>
                                                            <input required="" class="form-control square" name="adarea" id="adarea" type="text" value="<%=user[0].address[0].area%>">
                                                            <p class="error pt-1"></p>
                                                        </div>
                                                        <div class="form-group col-md-12">
                                                            <label>Town/City<span class="required">*</span></label>
                                                            <input required="" class="form-control square" name="adcity" id="adcity" type="text" value="<%=user[0].address[0].city%>">
                                                            <p class="error pt-1"></p>
                                                        </div>
                                                        <div class="form-group col-md-12">
                                                            <label>State<span class="required">*</span></label>
                                                            <input required="" class="form-control square" name="adstate" id="adstate" type="text" value="<%=user[0].address[0].state%>">
                                                            <p class="error pt-1"></p>
                                                        </div>
                                                        <div class="form-group col-md-12">
                                                            <label>Pincode <span class="required">*</span></label>
                                                            <input required="" class="form-control square" name="adpin" id="adpin" type="text" value="<%=user[0].address[0].pincode%>">
                                                            <p class="error pt-1"></p>
                                                        </div>
                                                        
                                                     <input type="text"  name="addressId" value="<%=user[0].address[0]._id%>" hidden>
                                                        <div class="col-md-12 text-center">
                                                            <button type="submit" class="btn btn-fill-out submit" name="submit" value="Submit" onclick="return checkEditAddressValidity()">Update</button>
                                                        </div>
                                                    </div>
                                                </form>
                                            </div>
                                        </div>
                                
                    </div>
                </div>
            </div>
        </section>
    </main>
